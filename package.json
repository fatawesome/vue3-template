{
  "name": "shell",
  "version": "1.0.0",
  "description": "",
  "main": "test.js",
  "scripts": {
    "build:client:dev": "cross-env NODE_ENV=development webpack --env dev --config build/webpack.client.config.js",
    "build:server:dev": "cross-env NODE_ENV=development webpack --env dev --config build/webpack.server.config.js",
    "build:dev": "npm run build:client:dev && npm run build:server:dev",
    "build:client:prod": "cross-env NODE_ENV=production webpack --env prod --config build/webpack.client.config.js",
    "build:server:prod": "cross-env NODE_ENV=production webpack --env prod --config build/webpack.server.config.js",
    "build:prod": "npm run build:client:prod && npm run build:server:prod",
    "serve:prod": "node ./server/index.js",
    "start:prod": "rimraf dist && npm run build:prod && npm run serve:prod",
    "watch:server": "cross-env NODE_ENV=development webpack --env dev --config build/webpack.server.config.js --progress --watch",
    "watch:client": "cross-env NODE_ENV=development webpack serve --env dev --config build/webpack.client.config.js --progress",
    "serve:dev": "nodemon --exec node ./server/index.js",
    "start": "rimraf dist && npm-run-all --parallel watch:client watch:server serve:dev"
  },
